{
  "compilerOptions": {
    "compilerOptions": {
      "types": [
        "node"
      ],
      "moduleResolution": "node"
    },
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/fetch": [
        "src/fetch/index"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    // "jsxImportSource": "@emotion/react",
    "noEmit": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
  },
  "include": [
    "src"
  ]
}